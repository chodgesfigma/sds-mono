{
  "name": "sds-web-components",
  "version": "0.0.1",
  "description": "Figma SDS replatforming, web components directory. This is the root and source of truth that all other output targets will be generated from. ",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/sds/sds.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "watch": "stencil build --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "scaffold:storybook": "tsx ./scripts/scaffold-component/index.ts",
    "scaffold": "run-s \"generate -- {@}\" \"scaffold:storybook -- {@}\" --",
    "icons:convert": "node ./scripts/generate-icon-files/convert-icons-from-react-to-stencil",
    "icons:docs": "node ./scripts/generate-icon-files/generate-iconography-story.js",
    "icons:rename": "tsx ./scripts/generate-icon-files/rename-icons-to-kebab-case.ts",
    "icons:figma": "tsx ./scripts/generate-icon-files/create-icons-figma-file",
    "scaffold:figma": "tsx ./scripts/scaffold-figma-connection",
    "dev:storybook": "npm-run-all build --parallel watch storybook",
    "lint:js": "eslint",
    "lint:js-fix": "eslint --fix",
    "lint:formatting": "prettier --check .",
    "lint:formatting-fix": "prettier --write .",
    "lint:styles": "npx stylelint ./**/*.scss",
    "lint:styles-fix": "npx stylelint ./**/*.scss --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "cd ../.. && husky packages/sds-web-components/.husky",
    "lint-staged": "npx lint-staged",
    "test-storybook": "test-storybook",
    "script:dev-resources": "cd scripts/dev-resources; node --env-file=../../.env app.mjs; cd -;"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@pxtrn/storybook-addon-docs-stencil": "^8.0.0",
    "@stencil-community/eslint-plugin": "^0.9.0",
    "@stencil/core": "^4.7.0",
    "@stencil/sass": "^3.0.12",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-webpack5-compiler-swc": "^2.0.0",
    "@storybook/blocks": "^8.5.3",
    "@storybook/html": "^8.5.3",
    "@storybook/html-webpack5": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@storybook/test-runner": "^0.21.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-storybook": "^0.11.2",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lint-staged": "^15.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "3.4.2",
    "puppeteer": "^21.9.0",
    "storybook": "^8.5.3",
    "stylelint": "^16.14.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "tsx": "^4.19.2",
    "typescript-eslint": "^8.23.0"
  },
  "license": "MIT",
  "dependencies": {
    "@figma/code-connect": "^1.3.1",
    "@shoelace-style/shoelace": "^2.20.0",
    "@stencil/angular-output-target": "^0.10.2",
    "@stencil/vue-output-target": "^0.9.4",
    "clsx": "^2.1.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
