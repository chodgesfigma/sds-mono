{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "lib": ["dom", "es2020"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "skipLibCheck": true,
    "types": [
      "react",
      "@stencil/core",
      "@stencil/react-output-target",
      "sds-web-components"
    ],
    "jsx": "react",
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules"]
}
